    OK
    <!-- --></button></div><script data-body="false" data-n-head="1" type="text/javascript">/* eslint-disable */
(function() {
  function getCookie(name) {
    var regex = new RegExp(`(^| )${name}=([^;]+)`);
    var match = document.cookie.match(regex);
    if (match) {
      return match[2];
    }
  }

  function getLangList() {
    return ['en-us', 'zh-cn', 'zh-tw', 'de-de', 'fr-fr', 'es-es', 'id-id', 'ja-jp', 'vi-vn', 'ko-kr', 'ru-ru', 'th-th', 'pt-pt'];
  }
  // 近似语言匹配
  function matchLanguage(lang) {
    if (/^en/i.test(lang)) return 'en-us';
    if (/^ja|jp/i.test(lang)) return 'ja-jp';
    if (/^ko/i.test(lang)) return 'ko-kr';
    if (/^zh-tw|tw/i.test(lang)) return 'zh-tw';
    if (/^es/i.test(lang)) return 'es-es';
    if (/^id/i.test(lang)) return 'id-id';
    if (/^ru/i.test(lang)) return 'ru-ru';
    if (/^th/i.test(lang)) return 'th-th';
    if (/^vi/i.test(lang)) return 'vi-vn';
    if (/^fr/i.test(lang)) return 'fr-fr';
    if (/^de/i.test(lang)) return 'de-de';
    if (/^pt/i.test(lang)) return 'pt-pt';
    if (/^zh/i.test(lang)) return 'zh-cn';
    return '';
  }
  function formatLang(lang, cookieLang, borowerFirstLang) {
    var LNAG_LIST_HEAD = getLangList();
    // 输入语言在规范列表中
    if (LNAG_LIST_HEAD.includes(lang)) {
      return {
        lang,
      };
    }
    // 输入语言在近似规范列表中
    if (LNAG_LIST_HEAD.includes(matchLanguage(lang))) {
      return {
        lang: matchLanguage(lang),
      };
    }
    // cookie语言在规范列表中
    if (LNAG_LIST_HEAD.includes(cookieLang)) {
      return {
        lang: cookieLang,
      };
    }
    // 浏览器语言在规则列表中
    if (LNAG_LIST_HEAD.includes(borowerFirstLang)) {
      return {
        lang: borowerFirstLang,
      };
    }
    // 浏览器语言在规则列表中
    if (LNAG_LIST_HEAD.includes(matchLanguage(borowerFirstLang))) {
      return {
        lang: matchLanguage(borowerFirstLang),
      };
    }
    return {
      lang: 'en-us',
    };
  }


  function isInvalidPath() {
    var LNAG_LIST_HEAD = getLangList();
    var urlPath = window.location.pathname.split('/').filter(v => !!v);

    return urlPath.length === 1 && !['home', 'character', 'company', 'news', 'world'].includes(urlPath[0]) && !LNAG_LIST_HEAD.includes(urlPath[0]);
  }
  function isRootUrl() {
    return location.pathname == '/';
  }

  if (isRootUrl() || isInvalidPath()) {
    var borowerFirstLang = (navigator.language || navigator.userLanguage).toLowerCase().split(',')[0];
    var cookieLang = getCookie('mi18nLang') || getCookie('hsrLang');
    var urlPath = window.location.pathname.split('/').filter(v => !!v);
    var { lang } = formatLang(urlPath[0], cookieLang, borowerFirstLang);
    location.href = `${location.protocol}//${location.host}/${lang}/${location.search}`;
  }
})();

</script></body></html>